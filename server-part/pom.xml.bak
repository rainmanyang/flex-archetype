	<profiles>
	  
	  <profile>
			<id>dev-env</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>		
			<properties>	
			    <jboss.client-jar.scope>provided</jboss.client-jar.scope>
				<jboss.client-jar.war.scope>compiled</jboss.client-jar.war.scope>
				<jboss.cluster.enabled>false</jboss.cluster.enabled>				
				<file.storage.path>c:/temp</file.storage.path>
				<wolfburg.memserver.url>0.0.0.0</wolfburg.memserver.url>
				<wolfburg.mail.switch.on>false</wolfburg.mail.switch.on>
				<scheduler.switch>false</scheduler.switch>
			</properties>
	  </profile>
	  <profile>
			<id>test-env</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>		
			<properties>	
			    <jboss.client-jar.scope>provided</jboss.client-jar.scope>
				<jboss.client-jar.war.scope>compiled</jboss.client-jar.war.scope>
				<jboss.cluster.enabled>false</jboss.cluster.enabled>				
				<file.storage.path>/mnt/wolfburgattachment</file.storage.path>
				<wolfburg.memserver.url>0.0.0.0</wolfburg.memserver.url>
				<wolfburg.mail.switch.on>false</wolfburg.mail.switch.on>
				<scheduler.switch>true</scheduler.switch>
			</properties>
	  </profile>
	  <profile>
			<id>uat-env</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>		
			<properties>	
			    <jboss.client-jar.scope>provided</jboss.client-jar.scope>
				<jboss.client-jar.war.scope>compiled</jboss.client-jar.war.scope>
				<jboss.cluster.enabled>false</jboss.cluster.enabled>				
				<file.storage.path>/mnt/wolfburgattachment</file.storage.path>
				<wolfburg.memserver.url>0.0.0.0</wolfburg.memserver.url>
				<wolfburg.mail.switch.on>true</wolfburg.mail.switch.on>
				<scheduler.switch>true</scheduler.switch>
			</properties>
	  </profile>
	  <profile>
			<id>prod-env</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>		
			<properties>	
			   <jboss.client-jar.scope>provided</jboss.client-jar.scope>
			   <jboss.client-jar.war.scope>compiled</jboss.client-jar.war.scope>
				<jboss.cluster.enabled>flase</jboss.cluster.enabled>				
				<file.storage.path>/mnt/wolfburgattachment</file.storage.path>
				<wolfburg.memserver.url>0.0.0.0</wolfburg.memserver.url>
				<wolfburg.mail.switch.on>true</wolfburg.mail.switch.on>
				<scheduler.switch>false</scheduler.switch>
			</properties>
		</profile>
		 <profile>
			<id>prod-report</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>		
			<properties>	
			   <jboss.client-jar.scope>provided</jboss.client-jar.scope>
			   <jboss.client-jar.war.scope>compiled</jboss.client-jar.war.scope>
				<jboss.cluster.enabled>flase</jboss.cluster.enabled>				
				<file.storage.path>/mnt/wolfburgattachment</file.storage.path>
				<wolfburg.memserver.url>0.0.0.0</wolfburg.memserver.url>
				<wolfburg.mail.switch.on>true</wolfburg.mail.switch.on>
				<scheduler.switch>true</scheduler.switch>
			</properties>
		</profile>
	</profiles>

